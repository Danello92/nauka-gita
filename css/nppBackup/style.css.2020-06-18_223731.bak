* {
	box-sizing: border-box;
	padding:0;
	margin:0;
}
body {
	background-color:#fffdd0;
	text-align: justify;
}

.container{
	display: flex;
	flex-wrap: wrap;
	justify-content: center;
	align-items: center;
	text-align: justify;
	margin: 0 96px 0 96px;
}
.container-inner {
	font-size:16px;
	font-family: 'Roboto', sans-serif;
}